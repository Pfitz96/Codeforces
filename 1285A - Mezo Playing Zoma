// ==================================================
// https://codeforces.com/problemset/problem/1285/A
// ==================================================

n = int(input())
s = input()
r = []
l = []
for i in s:
    if i == "R":
        r.append(i)
    else:
        l.append(i)
        
if len(s) - len(r) == 0:
    print(len(r) + 1)
else:
    print(len(r) + len(l) + 1)

